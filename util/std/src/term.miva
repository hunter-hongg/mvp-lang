color_red = (): string => "\x1b[0;31m"
color_green = (): string => "\x1b[0;32m"
color_yellow = (): string => "\x1b[0;33m"
color_blue = (): string => "\x1b[0;34m"
color_magenta = (): string => "\x1b[0;35m"
color_cyan = (): string => "\x1b[0;36m"
